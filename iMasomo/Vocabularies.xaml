<Window x:Class="iMasomo.Vocabularies"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Vocabularies" Height="300" Width="300" Loaded="Window_Loaded">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Label Content="Welcome to the Vocabularies Section"/>
            <Button Content="Next" Click="NextButton_Click"/>
            <Button Content="Previous" Click="PreviousButton_Click"/>
        </StackPanel>
        <Border BorderThickness="2">
            <Image Stretch="Fill" x:Name="imageHolder"/>
        </Border>
    </DockPanel>
</Window>
